{
  "version": 2,
  "framework": "express",
  "buildCommand": "npm install",
  "devCommand": "node index.js",
  "installCommand": "npm install",
  "outputDirectory": ".",
  "regions": ["iad1"],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        }
      ]
    }
  ]
}

